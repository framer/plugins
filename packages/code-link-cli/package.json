    "commander": "^14.0.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^22.19.2",
    "@types/ws": "^8.18.1",
    "tsdown": "^0.17.4",
    "tsx": "^4.21.0",
    "vitest": "^4.0.15"
  }
}
