{
  "name": "framer-code-link",
  "version": "0.3.0",
  "description": "CLI tool for syncing Framer code components - controller-centric architecture",
  "main": "dist/index.mjs",
  "type": "module",
  "bin": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "NODE_ENV=development tsx src/index.ts",
    "build": "tsdown src/index.ts",
